{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
		"MinimumLevel": "Verbose",
		"WriteTo": [
			{
				"Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:hh:mm:ss.fff} {Level:u3}] {Message:lj} {Properties}{NewLine}{Exception}"
                }
			}
		]
	},
    "OdospaceServer": {
        "Address": "127.0.0.1",
        "Port": 38000
    },
    "Browser": {
        "Browser": "Chrome",
        "Url": "http://home.loc/osd/123",
        "StubImage": "Resources\\VaultBoy.jpg",
        "StubImageError": "Resources\\VaultBoySad.jpg",
        "ReloadDelay": "00:01:00",
        "ScaleFactor": 3.0,
        "IsLandscape": true,
        "IsMobile": true
    },
    "Display": {
        "PageNo": 2,
        "Delay": 10000
    }
}
